{
  "name": "Dark Error Holes",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "src/js/deh_background.js",
      "src/js/deh_util.js"
    ],
    "persistent": true
  }
}
